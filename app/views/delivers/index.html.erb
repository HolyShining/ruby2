<p id="notice"><%= notice %></p>

<h1>Delivers</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Address</th>
      <th>Objectlist</th>
      <th>Delivers</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @delivers.each do |deliver| %>
      <tr>
        <td><%= deliver.name %></td>
        <td><%= deliver.address %></td>
        <td><%= deliver.objectlist %></td>
        <td><%= deliver.is_deliverd %></td>
        <td><%= link_to 'Show', deliver %></td>
        <td><%= link_to 'Edit', edit_deliver_path(deliver) %></td>
        <td><%= link_to 'Destroy', deliver, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Deliver', new_deliver_path %>
