<!DOCTYPE html>
<html>
  <head>
    <title>MBCourier</title>
    <%= csrf_meta_tags %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
    <script type="text/javascript" src="https://maps.googleapis.com/maps/api/js?key=AIzaSyCunL2-wTu_TVzO1lRRIyo-nth1v29MBtQ&libraries=places"></script>
    <script>
    $(document).ready(function(){
      $('select').formSelect();
    });
    $(function(){
      $("#autolocation").geocomplete();
    });
    </script>
  </head>

  <body>
    <%= render  "shared/navbar" %>

    <%= render 'shared/message' %>

    <div class="row">
    <div class="col m10 offset-m1">
    <%= yield %>
    </div>
    </div>
  </body>
</html>
