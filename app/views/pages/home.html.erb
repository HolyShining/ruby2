<br>
<% if(user_signed_in?) %>
<div class="row">
    <% @delivers.each do |deliver| %>
    <div class="col s6 m6">
      <div class="card blue-grey darken-1">
        <div class="card-content white-text">
          <span class="card-title"><%= deliver.name %></span>
          Content: <br/>
          <p><%= deliver.objectlist %></p>
          <hr>
          Address: <br/>
          <p><%= deliver.address %></p>
        </div>
        <div class="card-action">
          <%= link_to 'Done', delivers_done_path("id"=>deliver), method: :post %>
          <!--<%= link_to 'Done', :controller => 'delivers', :action => 'done', :item => deliver%>-->
        </div>
      </div>
    </div>
    <% end %>
</div>
<%= render "shared/floating_button"%>
<% else %>
<div class="row">
    <div class="col s12 m6">
      <div class="card red accent-2 darken-1">
        <div class="card-content white-text">
          <span class="card-title">Error!</span>
          <p>You need login before action</p>
        </div>
        <div class="card-action">
          <%= link_to  "Login", new_user_session_path %>
        </div>
      </div>
    </div>
  </div>
<% end %>


